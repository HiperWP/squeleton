            }
        }

        // Minimum Height
        function matchHeight() {

            // Array Classes
            // const classesToEqualize = ['min-line-1', 'min-line-2'];
            const classesToEqualize = [];
            for (let i = 1; i <= 20; i++) {
                classesToEqualize.push('min-line-' + i);
            }

            // Equalize Min-Height Elements
            function equalizeHeight(className) {
                const divs = document.querySelectorAll('.' + className);
                if (divs.length === 0) return;
                
                divs.forEach(div => div.style.minHeight = '');

                if (window.isXS) return;
                
                const maxHeight = Math.max(...Array.from(divs).map(div => 
                    div.getBoundingClientRect().height
                ));
                
                divs.forEach(div => div.style.minHeight = maxHeight + 'px');
            }

            classesToEqualize.forEach((className) => {
                equalizeHeight(className);
            });
        }

