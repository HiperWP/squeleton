&lt;script&gt;
document.addEventListener('DOMContentLoaded', function() {
    const {span, button} = van.tags
    const boxCounter3 = document.getElementById('box-counter-3');
    
    const Counter3 = () => {
        const counters = persistentGroup("counters", {
            likes: 0,
            deslikes: 0
        })
        
        return span(
