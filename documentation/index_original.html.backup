        // START RESIZE
        window.addEventListener('resize', function() {

            setTimeout(function () {
                matchHeight();
            }, 500);

        });

        // START DOM READY
        document.addEventListener('DOMContentLoaded', function() {
            "use strict";

            // WOW2 Animation
            var wow = new WOW({
                offset: 0,
                mobile: false,
                live: true
            });

            // WOW2 Init
            wow.init();

            // Match Height Init
            matchHeight();   
            
            // Scroll To Top
            var scrollTopArrow = document.querySelector('.scroll-top-arrow');

            if (scrollTopArrow) {
                scrollTopArrow.addEventListener('click', function(e) {
                    e.preventDefault();
                    window.scrollTo({
                        top: 0,
                        behavior: 'smooth'
                    });
                });
            }

            // Side Menu
            var showSideMenu = document.getElementById('showSideMenu');
            var sideMenuClose = document.getElementById('sideMenuClose');
            var siteMask = document.getElementById('site-mask');

            if (showSideMenu && sideMenuClose && siteMask) {
                showSideMenu.onclick = toggleMenu;
                sideMenuClose.onclick = toggleMenu;
                siteMask.onclick = toggleMenu;
            }

            // Side SubMenu
            var arrowSubMenu = document.querySelectorAll('.vertical-nav-inner .arrow-menu');

            arrowSubMenu.forEach(function(arrowMenu) {
                arrowMenu.addEventListener('click', function(e) {

                    e.preventDefault();
                    var parentDiv = e.target.closest('.menu-item-has-children');
                    if (parentDiv) {
                        parentDiv.classList.toggle('show-sub-menu');
                    }
                });
            });

            // Accordions
            var accordionMain = document.getElementById('accordion-main');
            if (accordionMain) {

                var accordions = document.querySelectorAll(".accordion-v1 .panel-heading");
                accordions.forEach(function(heading) {
                heading.addEventListener("click", function(e) {
                    e.preventDefault();
                
                    var panelCollapse = this.nextElementSibling;
                
                    accordions.forEach(function(accordion) {
                    var collapse = accordion.nextElementSibling;
                    if (collapse !== panelCollapse && collapse.classList.contains("panel-open")) {
                        collapse.classList.remove("panel-open");
                        accordion.classList.remove("panel-open");
                    }
                    });
                
                    if (panelCollapse.classList.contains("panel-open")) {
                    panelCollapse.classList.remove("panel-open");
                    this.classList.remove("panel-open");
                    } else {
                    panelCollapse.classList.add("panel-open");
                    this.classList.add("panel-open");
                    }
                });
                });

            }      

            // Toggles
            var toggleMain = document.getElementById('toggle-main');
            if (toggleMain) {

                var toggles = document.querySelectorAll(".toggles-v1 .panel-heading");
                toggles.forEach(function(heading) {
                heading.addEventListener("click", function(e) {
                    e.preventDefault();
                
                    var collapse = this.nextElementSibling;
                
                    if (collapse.classList.contains("panel-open")) {
                    collapse.classList.remove("panel-open");
                    this.classList.remove("panel-open");
                    } else {
                    collapse.classList.add("panel-open");
                    this.classList.add("panel-open");
                    }
                });
                });
                
            }

            // Veno Config Helpers
            const venoConfig = {
                bgcolor: '#000000',
                fitView: true,
                onPreOpen: () => {
                    document.body.style.paddingRight = (window.innerWidth - document.documentElement.clientWidth) + 'px';
                },
                onPreClose: () => {
                    document.body.style.paddingRight = '';
                }
            };

            // Veno Modal Classes
            const modals = ['.open-gallery', '.open-video', '.open-iframe'].map(selector => 
                new VenoBox({ ...venoConfig, selector })
            );

            // a11y-dialog Instance events
            var dialogContainer = document.querySelectorAll('.modal-dialog');
            dialogContainer.forEach(function(element) {

                // Do something when the dialog gets shown
                element.addEventListener('show', function (event) {
                    var scrollBarWidth = window.innerWidth - document.documentElement.clientWidth;
                    document.body.classList.add('modal-open');
                    document.body.style.paddingRight = scrollBarWidth + 'px';
                    document.body.style.overflow = 'hidden';
                })

                // Do something when the dialog gets hidden
                element.addEventListener('hide', function (event) {
                    document.body.classList.remove('modal-open');
                    document.body.style.paddingRight = '';
                    document.body.style.overflow = '';
                })
            });

            // Embla Init Slide
            document.querySelectorAll('.slide').forEach(sld_flixElement => {

                const sld_flixOpt = { 
                    loop: true,
                    slidesToScroll: 'auto',
                    skipSnaps: true
                }

                const sld_flixNode = sld_flixElement.querySelector('.slide__viewport')
                const sld_flixPlugs = [EmblaCarouselClassNames()]

                // Initialize the carousel
                const sld_flixApi = EmblaCarousel(sld_flixNode, sld_flixOpt, sld_flixPlugs)
                
            });

            // Counter-up2 Init Numbers
            var viewCounter = document.getElementById('view-counter');
            if (viewCounter) {
                const counterUp = window.counterUp.default;
                const counterCall = entries => {
                    entries.forEach( entry => {
                        const el = entry.target
                        if ( entry.isIntersecting && ! el.classList.contains('is-visible') ) {
                            for(const counter of counters) {
                                counterUp( counter, {
                                    duration: 1000,
                                    delay: 16,
                                })
                            el.classList.add('is-visible')
                            }
                        }
                    })
                }
                
                // Get all animated numbers
                const counters = document.querySelectorAll('.animated-number');

                // Intersection Observer
                const IO = new IntersectionObserver(counterCall, {threshold: 1} );
                IO.observe(viewCounter);
            }

            // Animated Elements
            const motionContainer = document.getElementById('motion');
            const demoHoverElements = motionContainer.querySelectorAll('.demo-hover');

            demoHoverElements.forEach(element => {
                element.addEventListener('mouseenter', () => {
                    element.classList.add('animated');
                });
                
                element.addEventListener('mouseleave', () => {
                    element.classList.remove('animated');
                });
            });

        });
    </script>

