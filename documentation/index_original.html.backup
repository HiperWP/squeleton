    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/prism.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/components/prism-markup.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/components/prism-css.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/components/prism-javascript.min.js"></script>

    <!-- Script para converter code-blocks antigos para Prism.js -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Função para decodificar HTML entities (&lt; → <, &gt; → >, etc.)
            const unescapeHTML = function(str) {
                const textarea = document.createElement('textarea');
                textarea.innerHTML = str;
                return textarea.value;
            };

            // Encontra todos os code-blocks que ainda usam HTML escapado
            document.querySelectorAll('.code-block').forEach(function(block) {
                // Se o bloco não tem <pre><code>, precisa converter
                if (!block.querySelector('pre')) {
                    let content = block.innerHTML.trim();

                    // Desescapa o HTML (&lt; vira <, &gt; vira >, etc)
                    content = unescapeHTML(content);

                    // Detecta a linguagem baseada no conteúdo
                    let language = 'markup'; // padrão para HTML
                    if (content.includes('class=') || content.includes('<div') || content.includes('<section')) {
                        language = 'markup';
                    } else if (content.includes('{') && content.includes(':') && content.includes(';')) {
                        language = 'css';
                    } else if (content.includes('function') || content.includes('const ') || content.includes('let ')) {
                        language = 'javascript';
                    }

                    // Cria a estrutura Prism
                    const pre = document.createElement('pre');
                    const code = document.createElement('code');
                    code.className = 'language-' + language;
                    code.textContent = content;
                    pre.appendChild(code);

                    // Substitui o conteúdo
                    block.innerHTML = '';
                    block.appendChild(pre);

                    // Reaplica o Prism highlight
                    Prism.highlightElement(code);
                }
            });
        });
    </script>

