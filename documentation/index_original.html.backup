&lt;!-- Container --&gt;
.d-flex             /* Display Flexbox */
.f-grid             /* Flexbox Grid (flex + wrap + row) - ideal para layouts em colunas */
.f-row              /* Direção horizontal */
.f-col              /* Direção vertical */
.f-wrap             /* Permitir quebra de linha */

&lt;!-- Alinhamento --&gt;
.f-items-center     /* Centralizar verticalmente */
.f-justify-center   /* Centralizar horizontalmente */
.f-justify-between  /* Espaçar entre elementos */
.f-justify-evenly   /* Espaçar igualmente */

&lt;!-- Alinhamento de múltiplas linhas (wrap) --&gt;
.f-content-start    /* Linhas no início */
.f-content-center   /* Linhas centralizadas */
.f-content-between  /* Espaço entre linhas */
.f-content-around   /* Espaço ao redor das linhas */

&lt;!-- Controle de itens --&gt;
.f-grow-1           /* Item cresce para preencher */
.f-shrink-0         /* Item não encolhe */
.f-self-center      /* Centralizar item individual */
.f-auto-max         /* flex: 1 1 auto (cresce e encolhe conforme necessário) */
.f-auto-min         /* flex: 0 1 auto (não cresce, apenas encolhe se necessário) */

&lt;!-- Gap --&gt;
.f-gap-0            /* Sem espaçamento */
.f-gap-15           /* Gap padrão de 15px */
.f-gap-20           /* Gap de 20px */
