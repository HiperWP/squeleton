                                    <tr>
                                        <td>xs-*</td>
                                        <td>Extra Small</td>
                                        <td>≤ 639px</td>
                                    </tr>
                                    <tr>
                                        <td>sm-*</td>
                                        <td>Small</td>
                                        <td>≥ 640px</td>
                                    </tr>
